{
  "version": 2,
  "builds": [
    {
      "src": "/src/server.js",
      "use": "@now/node-server",
      "config": {
        "includeFiles": [
          "**"
        ],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}